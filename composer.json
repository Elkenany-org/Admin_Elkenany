{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "8.0.*",
        "beyondcode/laravel-er-diagram-generator": "^2.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "~6.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^5.7",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.5",
        "mpociot/documentarian": "dev-master as 0.4.0",
        "mpociot/laravel-apidoc-generator": "dev-master",
        "nwidart/laravel-modules": "^8.2",
        "twilio/sdk": "^7.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files":[
            "app/Helpers/menuAndPermissions.php",
            "app/Helpers/NotificationConfig.php",
            "app/Helpers/help.php",
            "app/Helpers/AdsSystem.php",
            "app/Helpers/date_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
